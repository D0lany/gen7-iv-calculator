<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<VBox prefHeight="151.0" prefWidth="412.0" BorderPane.alignment="CENTER" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="be.lycoops.vincent.iv.calculator.additionalfiles.AdditionalfilesPresenter">
    <children>
        <TextField fx:id="routeDir" promptText="Route file directory">
            <VBox.margin>
                <Insets left="15.0" top="20.0" />
            </VBox.margin>
        </TextField>
        <TextField fx:id="pokemonDir" promptText="Pokemon file directory">
            <VBox.margin>
                <Insets left="15.0" top="10.0" />
            </VBox.margin>
        </TextField>
        <TextField fx:id="rangesDir" promptText="Ranges file directory">
            <VBox.margin>
                <Insets left="15.0" top="10.0" />
            </VBox.margin>
        </TextField>
        <Button fx:id="dirButton" mnemonicParsing="false" onAction="#updateDirs" text="Update directories">
            <VBox.margin>
                <Insets left="15.0" top="10.0" />
            </VBox.margin>
        </Button>
    </children>
</VBox>
