<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<HBox prefHeight="100.0" prefWidth="300.0" BorderPane.alignment="CENTER" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="be.lycoops.vincent.iv.calculator.configuration.ConfigurationPresenter">
    <children>

        <Button alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#levelMinus" prefHeight="60.0" prefWidth="60.0" style="-fx-font-size: 20px;" text="-">
            <HBox.margin>
                <Insets left="25.0" top="20.0" />
            </HBox.margin>
        </Button>
        <Label fx:id="level" alignment="CENTER" prefHeight="60.0" prefWidth="60.0" style="-fx-font-size: 20px;" text="L5" textAlignment="CENTER" VBox.vgrow="ALWAYS">
            <HBox.margin>
                <Insets top="20.0" />
            </HBox.margin>
        </Label>
        <Button fx:id="" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#levelPlus" prefHeight="60.0" prefWidth="60.0" style="-fx-font-size: 20px;" text="+">
            <HBox.margin>
                <Insets top="20.0" />
            </HBox.margin>
        </Button>
        <Button fx:id="evolved" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#evolve" prefHeight="60.0" prefWidth="60.0" text="Evo">
            <HBox.margin>
                <Insets left="10.0" top="20.0" />
            </HBox.margin>
            <font>
                <Font name="System Bold" size="13.0" />
            </font>
        </Button>
    </children>
    <BorderPane.margin>
        <Insets top="5.0" />
    </BorderPane.margin>
    <padding>
        <Insets left="3.0" />
    </padding>
</HBox>
